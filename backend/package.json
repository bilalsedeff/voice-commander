    "eventsource": "^4.0.0",
    "express": "^4.18.3",
    "express-rate-limit": "^7.4.0",
    "googleapis": "^161.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
